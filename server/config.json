{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port":5000,
  "remoting": {
    "context": false,
    "rest": {
      "supportedTypes": [
        "application/json"        
      ],
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb",
      "type": "application/*+json"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "domainSetting":{
    "host":"localhost:4000"
  }
}
